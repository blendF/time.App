    <script src="~/js/Timer.js"></script>
    <link rel="stylesheet" href="~/css/Timer.css" />


</head>
<body class="TimerBackground">

    <h1>Create</h1>

    <h4>TimeAndInterview</h4>
    <hr />
    <div class="row">
        <div class="col-md-12">
            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
